	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:html="org.integratedsemantics.flexibledashboardairmobile.html.*"
	verticalScrollPolicy="off" horizontalScrollPolicy="off"	
	width="100%" height="100%"> 
    
